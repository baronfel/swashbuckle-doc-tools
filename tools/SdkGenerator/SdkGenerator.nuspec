<?xml version="1.0"?>
<package >
    <metadata>
        <id>SdkGenerator</id>
        <version>1.0.0</version>
        <title>SdkGenerator</title>
        <authors>Ted Spence</authors>
        <owners>Ted Spence</owners>
        <license type="file">docs/LICENSE</license>
        <projectUrl>https://github.com/tspence/swashbuckle-doc-tools</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <readme>docs/README.md</readme>
        <description>
            A command line dotnet tool to retrieve swagger (OpenAPI) files and generate easy-to-use
            markdown documentation or friendly software development kit libraries. Can be automated
            using Github Workflows.
        </description>
        <summary>Command line dotnet tool to generate SDKs for your Swagger/OpenAPI project.</summary>
        <icon>docs/icons8-searchlight-90.png</icon>
        <releaseNotes>
            # 1.0.0
            July 27, 2023

            Made program automatable using verbs:
            * sdkgenerator create -p (new-project-file)
            * sdkgenerator build -p (project-file)
        </releaseNotes>
        <copyright>Copyright 2021 - 2023</copyright>
        <tags>SDK generator swagger openapi swashbuckle </tags>
        <repository type="git" url="https://github.com/tspence/swashbuckle-doc-tools" />
        <dependencies>
            <group targetFramework=".NETStandard2.0"/>
        </dependencies>
    </metadata>
    <files>
        <file src=".\LICENSE" target="docs/LICENSE"/>
        <file src=".\README.md" target="docs/README.md"/>
        <file src=".\icons8-searchlight-90.png" target="docs/icons8-searchlight-90.png"/>
        <file src=".\PatchNotes.md" target="docs/PatchNotes.md"/>
        <file src="src\Searchlight\bin\Release\netstandard2.0\*" target="lib\netstandard20" />
    </files>
</package>